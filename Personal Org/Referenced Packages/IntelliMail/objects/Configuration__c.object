    <validationRules>
        <fullName>Banner_Colour</fullName>
        <active>true</active>
        <errorConditionFormula>BEGINS(Banner_Colour__c, &quot;#&quot;)</errorConditionFormula>
        <errorDisplayField>Banner_Colour__c</errorDisplayField>
        <errorMessage>Please enter your 6 digit hexadecimal code without the # symbol</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Text_Colour</fullName>
        <active>true</active>
        <description>Ensures the text colour hexadecimal code doesn&apos;t start with a #</description>
        <errorConditionFormula>BEGINS(Text_Colour__c, &quot;#&quot;)</errorConditionFormula>
        <errorDisplayField>Text_Colour__c</errorDisplayField>
        <errorMessage>Please enter your 6 digit hexadecimal code without the # symbol</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Link_URL</fullName>
        <active>true</active>
        <description>ensures the footer link url starts with http:// or https://</description>
        <errorConditionFormula>NOT(OR(BEGINS(Footer_Link_Right__c, &quot;http://&quot;),BEGINS(Footer_Link_Right__c, &quot;https://&quot;), ISBLANK(Footer_Link_Right__c)))</errorConditionFormula>
        <errorDisplayField>Footer_Link_Right__c</errorDisplayField>
        <errorMessage>Please ensure your URL beings with http:// or https://</errorMessage>
    </validationRules>
